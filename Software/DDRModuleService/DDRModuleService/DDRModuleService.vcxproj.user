    <RemoteDebuggerWorkingDirectory>C:\www\bin\x64\Debug</RemoteDebuggerWorkingDirectory>
    <RemoteDebuggerServerName>192.168.0.95</RemoteDebuggerServerName>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
